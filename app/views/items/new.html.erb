<h1>Add an Item for location </h1>
<%= simple_form_for(@item) do |f| %>
  <%= f.input :category%>
  <%= f.input :name %>
  <%= f.input :picture, as: :file %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.submit class: "btn btn-primary" %>
<% end %>
 <%= link_to "back", items_path %>
